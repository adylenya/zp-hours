{
  "manifest_version": 3,
  "name": "Zimalab Projects Hours",
  "version": "1.1",
  "description": "Log hours to Zimalab projects directly from your browser.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://demo-zp.zimalab.com/*",
    "https://zp.zimalab.com/*",
    "http://127.0.0.1:8000/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}